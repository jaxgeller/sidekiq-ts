{
  "name": "sidekiq-ts",
  "version": "1.0.1",
  "description": "TypeScript client for Sidekiq job processing",
  "license": "MIT",
  "keywords": [
    "sidekiq",
    "redis",
    "job",
    "queue",
    "worker",
    "typescript"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "sidekiq": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=24.12.0"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "check": "npm run lint && npm run typecheck",
    "lint": "ultracite check",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "redis": "^5.10.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/node": "^25.0.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "ultracite": "^6.5.0",
    "vitest": "^4.0.16"
  }
}
